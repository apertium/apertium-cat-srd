<def-macro n="f_vaux_anar_inf" npar="2">
   <let><clip pos="1" side="tl" part="temps"/><lit-tag v="pii"/></let>
   <let><clip pos="2" side="tl" part="temps"/><lit-tag v="pp.m.sg"/></let>	<!-- FIXME: depèn del gènere i nombre del subjecte -->
